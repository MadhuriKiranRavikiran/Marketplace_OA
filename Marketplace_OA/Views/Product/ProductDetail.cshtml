﻿@model IEnumerable<Marketplace_OA.Models.ProductAttributesVM>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Search product", "Search")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProductsID)
            @Html.DisplayNameFor(model => model.AttributesID)
            @Html.DisplayNameFor(model => model.Attribute_Name)
            @Html.DisplayNameFor(model => model.Attribute_Value_ID)
            @Html.DisplayNameFor(model => model.Attribute_Value)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProductsID)
                @Html.DisplayFor(modelItem => item.AttributesID)
                @Html.DisplayFor(modelItem => item.Attribute_Name)
                @Html.DisplayFor(modelItem => item.Attribute_Value_ID)
                @Html.DisplayFor(modelItem => item.Attribute_Value)
            </td>
        </tr>
    }

</table>

